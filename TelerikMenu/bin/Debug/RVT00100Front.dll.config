<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL"
                 initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IRVT00100LogService" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_IRVT00100LogStreamingService" />
                <binding name="BasicHttpBinding_IRVT00100IncludeService" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_IRVT00100IncludeStreamingService" />
                <binding name="BasicHttpBinding_IRVT00100AppParam002Service"
                    messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_IRVT00100AppParam002StreamingService" />
                <binding name="BasicHttpBinding_IRVT00100StreamingService" />
                <binding name="BasicHttpBinding_IRVT00100FileService" messageEncoding="Mtom" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:7487/Services/RVT00100Service/RVT00100LogService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRVT00100LogService"
                contract="RVT00100LogServiceRef.IRVT00100LogService" name="BasicHttpBinding_IRVT00100LogService" />
            <endpoint address="http://localhost:7487/Services/RVT00100Service/RVT00100LogStreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRVT00100LogStreamingService"
                contract="RVT00100LogStreamingServiceRef.IRVT00100LogStreamingService"
                name="BasicHttpBinding_IRVT00100LogStreamingService" />
            <endpoint address="http://localhost:7487/Services/RVT00100Service/RVT00100IncludeService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRVT00100IncludeService"
                contract="RVT00100IncludeServiceRef.IRVT00100IncludeService"
                name="BasicHttpBinding_IRVT00100IncludeService" />
            <endpoint address="http://localhost:7487/Services/RVT00100Service/RVT00100IncludeStreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRVT00100IncludeStreamingService"
                contract="RVT00100IncludeStreamingServiceRef.IRVT00100IncludeStreamingService"
                name="BasicHttpBinding_IRVT00100IncludeStreamingService" />
            <endpoint address="http://localhost:7487/Services/RVT00100Service/RVT00100AppParam002Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRVT00100AppParam002Service"
                contract="RVT00100AppParam002ServiceRef.IRVT00100AppParam002Service"
                name="BasicHttpBinding_IRVT00100AppParam002Service" />
            <endpoint address="http://localhost:7487/Services/RVT00100Service/RVT00100AppParam002StreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRVT00100AppParam002StreamingService"
                contract="RVT00100AppParam002StreamingServiceRef.IRVT00100AppParam002StreamingService"
                name="BasicHttpBinding_IRVT00100AppParam002StreamingService" />
            <endpoint address="http://localhost:7487/Services/RVT00100Service/RVT00100StreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRVT00100StreamingService"
                contract="RVT00100StreamingServiceRef.IRVT00100StreamingService"
                name="BasicHttpBinding_IRVT00100StreamingService" />
            <endpoint address="http://localhost:7487/Services/RVT00100Service/RVT00100FileService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IRVT00100FileService"
                contract="RVT00100FileServiceRef.IRVT00100FileService" name="BasicHttpBinding_IRVT00100FileService" />
        </client>
    </system.serviceModel>
