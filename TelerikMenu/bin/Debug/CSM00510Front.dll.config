<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL"
                 initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICSM00510StreamingService" />
                <binding name="BasicHttpBinding_ICSM00510AssignmentStreamingService" />
                <binding name="BasicHttpBinding_ICSM00510Service" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICSM00510AssignmentService" messageEncoding="Mtom" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:7487/Services/CSM00510Service/CSM00510StreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00510StreamingService"
                contract="CSM00510StreamingServiceRef.ICSM00510StreamingService"
                name="BasicHttpBinding_ICSM00510StreamingService" />
            <endpoint address="http://localhost:7487/Services/CSM00510Service/CSM00510AssignmentStreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00510AssignmentStreamingService"
                contract="CSM00510AssignmentStreamingServiceRef.ICSM00510AssignmentStreamingService"
                name="BasicHttpBinding_ICSM00510AssignmentStreamingService" />
            <endpoint address="http://localhost:7487/Services/CSM00510Service/CSM00510Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00510Service"
                contract="CSM00510ServiceRef.ICSM00510Service" name="BasicHttpBinding_ICSM00510Service" />
            <endpoint address="http://localhost:7487/Services/CSM00510Service/CSM00510AssignmentService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00510AssignmentService"
                contract="CSM00510AssignmentServiceRef.ICSM00510AssignmentService"
                name="BasicHttpBinding_ICSM00510AssignmentService" />
        </client>
    </system.serviceModel>
</configuration>
