<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL"
                 initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ISAM01200Service" />
                <binding name="BasicHttpBinding_ISAM01200StreamingService" />
                <binding name="BasicHttpBinding_IUserCompanyService" />
                <binding name="BasicHttpBinding_IUserMenuService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:7487/Services/SAM01200Service/SAM01200Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ISAM01200Service"
                contract="SAM01200ServiceRef.ISAM01200Service" name="BasicHttpBinding_ISAM01200Service" />
            <endpoint address="http://localhost:7487/Services/SAM01200Service/SAM01200StreamingService/SAM01200StreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ISAM01200StreamingService"
                contract="SAM01200StreamingServiceRef.ISAM01200StreamingService"
                name="BasicHttpBinding_ISAM01200StreamingService" />
            <endpoint address="http://localhost:7487/Services/SAM01200Service/UserCompanyService/UserCompanyService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUserCompanyService"
                contract="UserCompanyServiceRef.IUserCompanyService" name="BasicHttpBinding_IUserCompanyService" />
            <endpoint address="http://localhost:7487/Services/SAM01200Service/UserMenuService/UserMenuService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUserMenuService"
                contract="UserMenuServiceRef.IUserMenuService" name="BasicHttpBinding_IUserMenuService" />
        </client>
    </system.serviceModel>
</configuration>
