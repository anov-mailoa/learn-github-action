<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL"
                 initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICSM00520Service" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICSM00520StreamingService" />
                <binding name="BasicHttpBinding_ICSM00520IssueStreamingService" />
                <binding name="BasicHttpBinding_ICSM00520IssueService" messageEncoding="Mtom" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:7487/Services/CSM00520Service/CSM00520Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00520Service"
                contract="CSM00520ServiceRef.ICSM00520Service" name="BasicHttpBinding_ICSM00520Service" />
            <endpoint address="http://localhost:7487/Services/CSM00520Service/CSM00520StreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00520StreamingService"
                contract="CSM00520StreamingServiceRef.ICSM00520StreamingService"
                name="BasicHttpBinding_ICSM00520StreamingService" />
            <endpoint address="http://localhost:7487/Services/CSM00520Service/CSM00520IssueStreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00520IssueStreamingService"
                contract="CSM00520IssueStreamingServiceRef.ICSM00520IssueStreamingService"
                name="BasicHttpBinding_ICSM00520IssueStreamingService" />
            <endpoint address="http://localhost:7487/Services/CSM00520Service/CSM00520IssueService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00520IssueService"
                contract="CSM00520IssueServiceRef.ICSM00520IssueService" name="BasicHttpBinding_ICSM00520IssueService" />
        </client>
    </system.serviceModel>
</configuration>
