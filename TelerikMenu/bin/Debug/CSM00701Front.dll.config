<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL"
                 initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICSM00701SchemaService" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICSM00701Service" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICSM00701StreamingService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:7487/Services/CSM00701Service/CSM00701SchemaService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00701SchemaService"
                contract="CSM00701SchemaServiceRef.ICSM00701SchemaService"
                name="BasicHttpBinding_ICSM00701SchemaService" />
            <endpoint address="http://localhost:7487/Services/CSM00701Service/CSM00701Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00701Service"
                contract="CSM00701ServiceRef.ICSM00701Service" name="BasicHttpBinding_ICSM00701Service" />
            <endpoint address="http://localhost:7487/Services/CSM00701Service/CSM00701StreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00701StreamingService"
                contract="CSM00701StreamingServiceRef.ICSM00701StreamingService"
                name="BasicHttpBinding_ICSM00701StreamingService" />
        </client>
    </system.serviceModel>
</configuration>
