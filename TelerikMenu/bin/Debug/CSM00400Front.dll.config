<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL"
                 initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICSM00300Service" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICSM00300StreamingService" />
                <binding name="BasicHttpBinding_ICSM00400Service" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICSM00400StreamingService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:7487/Services/CSM00300Service/CSM00300Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00300Service"
                contract="CSM00300ServiceRef.ICSM00300Service" name="BasicHttpBinding_ICSM00300Service" />
            <endpoint address="http://localhost:7487/Services/CSM00300Service/CSM00300StreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00300StreamingService"
                contract="CSM00300StreamingServiceRef.ICSM00300StreamingService"
                name="BasicHttpBinding_ICSM00300StreamingService" />
            <endpoint address="http://localhost:7487/Services/CSM00400Service/CSM00400Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00400Service"
                contract="CSM00400ServiceRef.ICSM00400Service" name="BasicHttpBinding_ICSM00400Service" />
            <endpoint address="http://localhost:7487/Services/CSM00400Service/CSM00400StreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00400StreamingService"
                contract="CSM00400StreamingServiceRef.ICSM00400StreamingService"
                name="BasicHttpBinding_ICSM00400StreamingService" />
        </client>
    </system.serviceModel>
</configuration>
