                <binding name="BasicHttpBinding_ICSM00600DetailStreamingService" />
                <binding name="BasicHttpBinding_ICSM00600Service" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICSM00600StreamingService" />
