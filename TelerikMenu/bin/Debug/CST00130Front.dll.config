<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL"
                 initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>

    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICST00130StreamingService" />
                <binding name="BasicHttpBinding_ICST00130Service" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICST00130FileService" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICST00130Service1" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICST00130StreamingService1" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:7487/Services/CST00130Service/CST00130StreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICST00130StreamingService"
                contract="CST00130StreamingServiceRef.ICST00130StreamingService"
                name="BasicHttpBinding_ICST00130StreamingService" />
            <endpoint address="http://localhost:7487/Services/CST00130Service/CST00130Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICST00130Service"
                contract="CST00130ServiceRef.ICST00130Service" name="BasicHttpBinding_ICST00130Service" />
            <endpoint address="http://localhost:7487/Services/CST00130Service/CST00130FileService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICST00130FileService"
                contract="CST00130FileServiceRef.ICST00130FileService" name="BasicHttpBinding_ICST00130FileService" />
            <endpoint address="http://localhost:7487/Services/CST00130Service/CST00130Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICST00130Service1"
                contract="CST00130ServiceRef.ICST00130Service" name="BasicHttpBinding_ICST00130Service1" />
            <endpoint address="http://localhost:7487/Services/CST00130Service/CST00130StreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICST00130StreamingService1"
                contract="CST00130StreamingServiceRef.ICST00130StreamingService"
                name="BasicHttpBinding_ICST00130StreamingService1" />
        </client>
    </system.serviceModel>
</configuration>
