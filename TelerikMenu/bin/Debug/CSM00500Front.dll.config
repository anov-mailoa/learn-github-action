<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL"
                 initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICSM00500UsersService" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICSM00500UsersStreamingService" />
                <binding name="BasicHttpBinding_ICSM00500SessionStreamingService" />
                <binding name="BasicHttpBinding_ICSM00500SessionService" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICSM00500Service" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICSM00500StreamingService" />
                <binding name="BasicHttpBinding_ICSM00500ItemStreamingService" />
                <binding name="BasicHttpBinding_ICSM00500ItemService" messageEncoding="Mtom" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:7487/Services/CSM00500Service/CSM00500UsersService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00500UsersService"
                contract="CSM00500UsersServiceRef.ICSM00500UsersService" name="BasicHttpBinding_ICSM00500UsersService" />
            <endpoint address="http://localhost:7487/Services/CSM00500Service/CSM00500UsersStreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00500UsersStreamingService"
                contract="CSM00500UsersStreamingServiceRef.ICSM00500UsersStreamingService"
                name="BasicHttpBinding_ICSM00500UsersStreamingService" />
            <endpoint address="http://localhost:7487/Services/CSM00500Service/CSM00500SessionStreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00500SessionStreamingService"
                contract="CSM00500SessionStreamingServiceRef.ICSM00500SessionStreamingService"
                name="BasicHttpBinding_ICSM00500SessionStreamingService" />
            <endpoint address="http://localhost:7487/Services/CSM00500Service/CSM00500SessionService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00500SessionService"
                contract="CSM00500SessionServiceRef.ICSM00500SessionService"
                name="BasicHttpBinding_ICSM00500SessionService" />
            <endpoint address="http://localhost:7487/Services/CSM00500Service/CSM00500Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00500Service"
                contract="CSM00500ServiceRef.ICSM00500Service" name="BasicHttpBinding_ICSM00500Service" />
            <endpoint address="http://localhost:7487/Services/CSM00500Service/CSM00500StreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00500StreamingService"
                contract="CSM00500StreamingServiceRef.ICSM00500StreamingService"
                name="BasicHttpBinding_ICSM00500StreamingService" />
            <endpoint address="http://localhost:7487/Services/CSM00500Service/CSM00500ItemStreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00500ItemStreamingService"
                contract="CSM00500ItemStreamingServiceRef.ICSM00500ItemStreamingService"
                name="BasicHttpBinding_ICSM00500ItemStreamingService" />
            <endpoint address="http://localhost:7487/Services/CSM00500Service/CSM00500ItemService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00500ItemService"
                contract="CSM00500ItemServiceRef.ICSM00500ItemService" name="BasicHttpBinding_ICSM00500ItemService" />
        </client>
    </system.serviceModel>
</configuration>
