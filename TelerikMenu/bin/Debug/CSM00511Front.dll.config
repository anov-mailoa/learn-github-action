<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information" />
        </switches>
        <sharedListeners>
            <add name="FileLog"
                 type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL"
                 initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICSM00511StreamingService" />
                <binding name="BasicHttpBinding_ICSM00511AssignmentStreamingService" />
                <binding name="BasicHttpBinding_ICSM00511AssignmentService" messageEncoding="Mtom" />
                <binding name="BasicHttpBinding_ICSM00511Service" messageEncoding="Mtom" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:7487/Services/CSM00511Service/CSM00511StreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00511StreamingService"
                contract="CSM00511StreamingServiceRef.ICSM00511StreamingService"
                name="BasicHttpBinding_ICSM00511StreamingService" />
            <endpoint address="http://localhost:7487/Services/CSM00511Service/CSM00511AssignmentStreamingService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00511AssignmentStreamingService"
                contract="CSM00511AssignmentStreamingServiceRef.ICSM00511AssignmentStreamingService"
                name="BasicHttpBinding_ICSM00511AssignmentStreamingService" />
            <endpoint address="http://localhost:7487/Services/CSM00511Service/CSM00511AssignmentService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00511AssignmentService"
                contract="CSM00511AssignmentServiceRef.ICSM00511AssignmentService"
                name="BasicHttpBinding_ICSM00511AssignmentService" />
            <endpoint address="http://localhost:7487/Services/CSM00511Service/CSM00511Service.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICSM00511Service"
                contract="CSM00511ServiceRef.ICSM00511Service" name="BasicHttpBinding_ICSM00511Service" />
        </client>
    </system.serviceModel>
</configuration>
